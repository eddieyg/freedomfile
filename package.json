{
  "name": "freedomfile",
  "version": "1.0.0",
  "description": "Freedom to use PDF, DOC and other document processing",
  "repository": {
    "type": "git",
    "url": "https://github.com/eddieyg/freedomfile.git"
  },
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "scripts": {
    "commit": "git add . & npx cz",
    "build": "tsup",
    "test": "vitest run",
    "test:w": "vitest watch",
    "coverage": "vitest run --coverage",
    "publish:pre": "pnpm test & pnpm build"
  },
  "dependencies": {
    "fs-extra": "^10.1.0",
    "pdf-lib": "^1.17.1",
    "sharp": "^0.30.7"
  },
  "devDependencies": {
    "@types/node": "^18.6.3",
    "commitizen": "^4.2.5",
    "cz-emoji-conventional": "^1.0.1",
    "tsup": "^6.2.1",
    "vitest": "^0.19.1"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-emoji-conventional"
    }
  },
  "keywords": [
    "image transfer pdf",
    "pdf typeset",
    "pdf a4 transfer a3",
    "document",
    "pdf排版",
    "pdf小册子排版处理",
    "pdf A3",
    "小册子"
  ],
  "author": "eddieyg",
  "email": "eddiezhao@foxmail.com",
  "license": "MIT"
}
